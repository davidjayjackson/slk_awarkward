```
#!/usr/bin/gawk -f

BEGIN {
  FS = ","   # Set the field separator to a comma (assuming CSV input)
}

{
  artist = $1  # Assuming the artist name is in the first field
  
  if (artist in count) {
    count[artist] += 1  # Increment the count for the artist
  } else {
    count[artist] = 1  # Initialize the count for a new artist
  }
}

END {
  # Store the counts and artist names in an array
  for (artist in count) {
    songs[count[artist]] = artist
  }

  # Sort the counts in descending order
  num_counts = asorti(songs, sorted_counts, "@ind_num_desc")

  # Print the results
  for (i = 1; i <= num_counts; i++) {
    count = sorted_counts[i]
    artist = songs[count]
    print artist, count
  }
}


```
